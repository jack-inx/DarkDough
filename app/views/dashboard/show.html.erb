<div id="dashboard">

  <aside>
    <div class="left-side">
      <div id="current_user">
        <%= full_name current_user %>
      </div>
      <%= acumen_test_menu(current_user, current_page_menu('acumen_tests')) %>
    </div>
    <div class="left-side">
      <h3>Top users</h3>
    </div>

    <div class="left-side">
      <h3>Videos</h3>
    </div>

    <div class="left-side">
      <h3>Articles</h3>
    </div>
  </aside>

  <div id="user-info">
    <div class="block right">
      <%= render 'goals', :goals => @goals %>
      <%= render 'budgets', :budgets => @budgets %>
      <%= render 'historical_spending', :spending => @spending_transactions %>
    </div>
  </div>

</div>

<%= javascript_tag do  %>
  $('.dashboard.budgets').progressBar();
<% end %>
<div id="by_merchant_partial">
  <div class="spending_container svg bubble-info">
    <% if @spending_transactions.any? %>
      <% merchanized(@spending_transactions).map do |key, val| %>
        <%= key %> &ndash; <%= val %><br />
      <% end %>
    <% else %>
      You do not have any spending transactions.
    <% end %>
    <div class="hidden spending json"><%= @spending_by_merchant_json.to_json %></div>
  </div>

  <div class="income_container svg bubble-info by_category">
    <% if @income_transactions.any? %>
      <% merchanized(@income_transactions).map do |key, val| %>
        <%= key %> &ndash; <%= val %><br />
      <% end %>
    <% else %>
      You do not have any income transactions.
    <% end %>
    <div class="hidden income json"><%= @income_by_merchant_json.to_json %></div>
  </div>

  <div class="net_income_containe svg bubble-info by_category" style="vertical-align: top;">
    No 'Net Income' for Filter "By merchant"
  </div>

  <div class="assets_containe svg bubble-info by_category">
    No assets now
  </div>

  <div class="debts_containe svg bubble-info by_category">
    No debts now
  </div>

  <div class="net_worth_containe svg bubble-info by_category">
    No Net Worth now
  </div>

</div>

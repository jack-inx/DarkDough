$("#result").html("<%= escape_javascript(render :partial => "budgets") %>");

$('#create_new_budget').dialog({
  autoOpen: false,
  resizable: false,
  modal: true,
  minWidth: 490,
  dialogClass: 'my_dialog',
  title: "New budget",
  buttons: {
    'Save': function() {
      $('#new_budget').submit()
      $(this).dialog('close')
    },
    'Cancel': function() {
      $(this).dialog('close')
    }
  }
})

$('.new-budget').on("click", function() {
  $('#create_new_budget').dialog("open");
  return false;
});

$('#result').progressBar();